<table id="tbl" class="table table-striped table-bordered dt-responsive nowrap" style="width:100%;">
              <thead>
              <tr>
                <th>Repository</th>
                <th>Description</th>
                <th>Date Updated</th>
              </tr>
              </thead>
              <tbody>
                  <% @searchrepo['items'].each do |repo| %>
                      <tr>
                      <td><%= repo['full_name'] %></td>
                      <td><%= repo['description'] %></td>
                      <td><%= repo['updated_at'] %></td>
                      </tr>
                  <% end %>
              </tbody>
            </table>